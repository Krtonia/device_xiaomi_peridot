<resources>
    <!-- Configuring priorities of A2DP source codecs. Larger value means
         higher priority. Value -1 means the codec is disabled.
         Value 0 is reserved and should not be used here. Enabled codecs
         should have priorities in the interval [1, 999999], and each priority
         value should be unique. -->
    <integer name="a2dp_source_codec_priority_sbc">1001</integer>
    <integer name="a2dp_source_codec_priority_aac">2001</integer>
    <integer name="a2dp_source_codec_priority_aptx">3001</integer>
    <integer name="a2dp_source_codec_priority_aptx_hd">4001</integer>
    <integer name="a2dp_source_codec_priority_lc3">5001</integer>
    <integer name="a2dp_source_codec_priority_ldac">6001</integer>
    <integer name="a2dp_source_codec_priority_aptx_adaptive">7001</integer>
    <integer name="a2dp_source_codec_priority_aptx_tws">8001</integer>
    <integer name="a2dp_source_codec_priority_lhdcv2">9001</integer>
    <integer name="a2dp_source_codec_priority_lhdcv3">9002</integer>
    <integer name="a2dp_source_codec_priority_lhdcv5">9003</integer>

    <!-- max priority is used to dynamically increase the priority
         of codecs(mainly Aptx Adaptive) to highest value based
         on config
         Value of a2dp_source_codec_priority_max should be set to
         1000 + priority of codec with highest priority-->
    <integer name="a2dp_source_codec_priority_max">10003</integer>
</resources>
